{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "curly": [true, "ignore-same-line"],
        "max-classes-per-file": false,
        "max-line-length": false,
        "member-access": false,
        "object-literal-sort-keys": false,
        "quotemark": [true, "avoid-escape"]
    },
    "rulesDirectory": []
}